fresco {
  env = "prod"

  routes {
    # If ask takes more time than this to complete the request is failed
    ask-timeout = 5s
  }

  prod {
    aws {
      accessKey = "accessKey"
      secretKey = "secretKey"
      storage {
        recipesTableName = "recipes"
        ingredientsTableName = "ingredients"
        bucketName = "frescoBucket"
      }
    }
  }
}
